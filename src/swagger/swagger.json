				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/IHealthStatus"
								},
								"examples": {
									"Example 1": {
										"value": {
											"status": "ok",
											"message": "the server is healthy",
											"version": "2.1.5"
										}
									}
								}
							}
						}
					}
				},
				"description": "Check server status whether it is alive or not",
				"security": [],
				"parameters": []
			}
		},
